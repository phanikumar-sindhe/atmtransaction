#%RAML 1.0 Trait
queryParameters:
      customerName:
        type: string
        required: true
        maxLength: 45
        minLength: 4
      bank:
        type: string
        enum:
          - ICICI
          - AXIS
          - HDFC
        required: true
      accountType:
        type: string
        enum:
          - savings
          - current
        required: true
      branchName:
        type: string
        required: true
        minLength: 4
        maxLength: 10